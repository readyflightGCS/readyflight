{
  "name": "readyflight",
  "version": "0.0.01",
  "description": "An Electron application with React and TypeScript",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "private": true,
  "packageManager": "bun@1",
  "workspaces": [
    "client-frontend",
    "client-backend",
    "electron",
    "docs",
    "libs"
  ],
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "cd client-frontend && bun run build --dry-run",
    "typecheck:web": "cd client-frontend && bun run typecheck",
    "typecheck": "bun run typecheck:node && bun run typecheck:web",
    "dev": "cd electron && bun run dev",
    "dev:electron": "cd electron && bun run dev",
    "dev:web": "cd client-frontend && bun run dev",
    "dev:backend": "cd client-backend && bun run dev",
    "docs:website": "cd docs && bun run dev:website",
    "start": "cd electron && bun run preview",
    "build": "cd electron && bun run build",
    "build:unpack": "cd electron && bun run build:unpack",
    "build:win": "cd electron && bun run build:win",
    "build:mac": "cd electron && bun run build:mac",
    "build:linux": "cd electron && bun run build:linux"
  },
  "devDependencies": {
    "@types/bun": "^1.3.0",
    "@types/leaflet": "^1.9.21",
    "eslint": "^9.0.0",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "leaflet": "^1.9.4",
    "react-leaflet": "^5.0.0-rc.2",
    "use-sync-external-store": "^1.6.0"
  }
}
